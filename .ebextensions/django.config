option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: timetable_solutions.base_files.settings.production
    PYTHONPATH: "/var/app/current:$PYTHONPATH"
  aws:elasticbeanstalk:container:python:
    WSGIPath: timetable_solutions.base_files.wsgi:application
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: static

container_commands:
 collectstatic:
    command: "source /var/app/venv/*/bin/activate && python3 timetable_solutions/manage.py collectstatic --noinput"
